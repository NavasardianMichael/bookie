{
  "name": "bookie",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack --port 4141",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint-fix": "next lint --fix",
    "format": "cd src && npx prettier --write .",
    "prepare": "husky",
    "mock-api": "tsx ./src/mock/server.ts",
    "watch": "concurrently --kill-others-on-fail -n app,api -c green,cyan \"kill-port 3000\" \"kill-port 3001\" \"pnpm dev\" \"pnpm mock-api\"",
    "cleardeps": "rm -rf node_modules",
    "cleardeps:hard": "rm -rf node_modules && rm -rf package-lock.json && rm -rf pnpm-lock.yaml && rm -rf yarn.lock"
  },
  "dependencies": {
    "@ant-design/icons": "^6.1.0",
    "@ant-design/nextjs-registry": "^1.3.0",
    "@fullcalendar/core": "^6.1.20",
    "@fullcalendar/daygrid": "^6.1.20",
    "@fullcalendar/interaction": "^6.1.20",
    "@fullcalendar/list": "^6.1.20",
    "@fullcalendar/react": "^6.1.20",
    "@fullcalendar/timegrid": "^6.1.20",
    "antd": "^6.2.1",
    "antd-img-crop": "^4.27.0",
    "axios": "^1.13.2",
    "dayjs": "^1.11.19",
    "formik": "^2.4.9",
    "immer": "^11.1.3",
    "json-server": "1.0.0-beta.3",
    "libphonenumber-js": "^1.12.34",
    "next": "^16.1.4",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-icons": "^5.5.0",
    "react-world-flags": "^1.6.0",
    "serialize-javascript": "^7.0.2",
    "yup": "^1.7.1",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@next/eslint-plugin-next": "^16.1.4",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/json-server": "^0.14.8",
    "@types/node": "^22.15.30",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "@types/serialize-javascript": "^5.0.4",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.26",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "express": "^5.2.1",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "kill-port": "^2.0.1",
    "prettier": "^3.8.0",
    "react-hooks": "^1.0.1",
    "schema-dts": "^1.1.5",
    "tailwindcss": "^4.1.18",
    "ts-node": "^10.9.2",
