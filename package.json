{
  "name": "bookie",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack --port 3000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint-fix": "next lint --fix",
    "format": "cd src && npx prettier --write .",
    "prepare": "husky",
    "mock-api": "tsx ./src/mock/server.ts",
    "watch": "concurrently --kill-others-on-fail -n app,api -c green,cyan \"kill-port 3000\" \"kill-port 3001\" \"pnpm dev\" \"pnpm mock-api\"",
    "cleardeps": "rm -rf node_modules",
    "cleardeps:hard": "rm -rf node_modules && rm -rf package-lock.json && rm -rf pnpm-lock.yaml && rm -rf yarn.lock"
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "@ant-design/nextjs-registry": "^1.1.0",
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "@fullcalendar/core": "^6.1.18",
    "@fullcalendar/daygrid": "^6.1.18",
    "@fullcalendar/interaction": "^6.1.18",
    "@fullcalendar/list": "^6.1.18",
    "@fullcalendar/react": "^6.1.18",
    "@fullcalendar/timegrid": "^6.1.18",
    "antd": "^5.26.6",
    "axios": "^1.11.0",
    "dayjs": "^1.11.13",
    "formik": "^2.4.6",
    "immer": "^10.1.1",
    "json-server": "1.0.0-beta.3",
    "libphonenumber-js": "^1.12.10",
    "next": "15.2.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-world-flags": "^1.6.0",
    "serialize-javascript": "^6.0.2",
    "yup": "^1.6.1",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@next/eslint-plugin-next": "^15.4.3",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/json-server": "^0.14.8",
    "@types/node": "^20.19.9",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/serialize-javascript": "^5.0.4",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "concurrently": "^9.2.0",
    "cors": "^2.8.5",
    "eslint": "^9.31.0",
    "eslint-config-next": "15.2.3",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "express": "^5.1.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "kill-port": "^2.0.1",
    "prettier": "^3.6.2",
    "react-hooks": "^1.0.1",
    "schema-dts": "^1.1.5",
    "tailwindcss": "^4.1.11",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}
